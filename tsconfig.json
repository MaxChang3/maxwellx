{
    "compilerOptions": {
        "target": "esnext",
        "allowJs": false,
        "useDefineForClassFields": true,
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noEmit": false,
        "declaration": true,
    },
    "exclude": [
        "node_modules"
    ]
}